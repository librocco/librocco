		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"prettier": "^2.6.2",
		"typescript": "^4.8.4",
		"uuid": "~8.3.2",
		"vite": "~4.0.0",
		"vitest": "~0.28.5",
		"rxjs": "~7.8.0",
		"luxon": "~3.3.0",
