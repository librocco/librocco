	"main": "./dist/index.js",
	"exports": {
		".": {
			"import": "./dist/index.es.js",
			"require": "./dist/index.js",
			"default": "./dist.index.es.js"
		}
	},
	"types": "dist/index.d.ts",
	"scripts": {
