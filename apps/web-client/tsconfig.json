{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"rootDirs": ["./.svelte-kit/types", "./src"],
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"strict": true,
		"noEmit": true
	}
}
