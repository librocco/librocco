		"@melt-ui/pp": "~0.1.4",
		"svelte-sequential-preprocessor": "~2.0.1",
		"sveltekit-superforms": "~1.10.2",
		"zod": "~3.22.2",
		"@vitest/ui": "~1.1.0"
