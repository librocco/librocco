{
	"name": "@librocco/web-client",
	"version": "0.0.1",
	"scripts": {
		"start": "vite dev",
		"build": "echo \"Skipping web app build as part of rush's bulk 'build' script. To build the app for production run: 'rushx build:prod'\"",
		"build:prod": "vite build",
		"preview": "vite preview",
		"test": "vitest --ui",
		"test:ci": "vitest --",
		"typecheck": "svelte-check --tsconfig ./tsconfig.json --threshold warning",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"lint:strict": "prettier --check --plugin-search-dir=. . && eslint . --max-warnings=0",
		"format": "prettier --write --plugin-search-dir=. .",
		"postinstall": "svelte-kit sync",
		"story:dev": "histoire dev",
		"story:build": "histoire build",
		"story:preview": "histoire preview"
	},
	"devDependencies": {
		"@histoire/plugin-svelte": "~0.15.8",
		"@histoire/controls": "~0.11.3",
		"@librocco/book-data-extension": "workspace:*",
		"@librocco/shared": "workspace:*",
		"@sveltejs/kit": "~1.15.7",
		"@testing-library/dom": "~8.20.0",
		"@testing-library/jest-dom": "~5.16.5",
		"@testing-library/svelte": "~3.1.3",
		"@testing-library/user-event": "~14.4.3",
		"@typescript-eslint/eslint-plugin": "~5.27.0",
		"@typescript-eslint/parser": "~5.27.0",
		"@types/testing-library__jest-dom": "~5.14.5",
		"@types/pouchdb": "~6.4.0",
		"eslint": "~8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"histoire": "~0.15.8",
		"postcss": "~8.4.13",
		"prettier": "~2.7.1",
		"prettier-plugin-tailwindcss": "~0.1.13",
		"pouchdb-adapter-memory": "~8.0.1",
		"svelte": "~3.58.0",
		"svelte-check": "^3.0.0",
		"svelte-preprocess": "^5.0.3",
		"tailwindcss": "~3.2.4",
		"tslib": "^2.3.1",
		"typescript": "^4.8.4",
		"vite": "~4.0.0",
		"@sveltejs/adapter-static": "^2.0.2",
		"vitest": "~1.1.0",
